<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>

  <display-name>MyWebApp</display-name>

  <servlet>
    <servlet-name>CreateDeveloperServlet</servlet-name>
    <servlet-class>controller.developer.CreateDeveloperServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ListDeveloperServlet</servlet-name>
    <servlet-class>controller.developer.ListDevelopersServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>CreateDeveloperServlet</servlet-name>
    <url-pattern>/add-developer</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>ListDeveloperServlet</servlet-name>
    <url-pattern>/list-developers</url-pattern>
  </servlet-mapping>


</web-app>
